require("dotenv").config();
const express = require("express");

const app = express();

const port = 3000;
const appName = process.env.NUMBER;

app.get("/", (req, res) => {
  res.send("{{ hello_world }} " + appName);
});

app.listen(port, () => {
  console.log(`Example app listening on port ${port}`);
});
